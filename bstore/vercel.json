{
    "builds": [
      {
        "src": "bstore/wsgi.py", 
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/home/app/web/Bulux/bstore/static/$1" 
      },
      {
        "src": "/(.*)",
        "dest": "bstore/wsgi.py"
      }
    ],
  
    "env": {
      "STATIC_ROOT": "/home/app/web/Bulux/bstore/static"  
    }
  }