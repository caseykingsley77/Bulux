{
    "builds": [ 
      {
        "src": "bstore/wsgi.py",
        "use": "@vercel/python",  
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
  
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/home/app/web/Bulux/bstore/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "bstore/wsgi.py"
      }
    ],
  
    "env": {
      "DJANGO_SETTINGS_MODULE": "bstore.settings",
      "STATIC_ROOT": "/home/app/web/Bulux/bstore/static"
    } 
  }